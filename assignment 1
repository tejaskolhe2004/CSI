n=10

# Lower Triangular Pattern
print("Lower Triangle:")
for i in range(1, n + 1):
    print("*" * i)

print("\nUpper Triangle:")
# Upper Triangular Pattern
for i in range(n, 0, -1):
    print("*" * i)

print("\nPyramid:")
# Pyramid Pattern
for i in range(1, n + 1):
    print(" " * (n - i) + "*" * (2 * i - 1))
  
